        "@grpc/reflection": "^1.0.4"
    },
    "devDependencies": {
        "@types/node": "^25.0.3",
        "grpc-tools": "^1.13.1",
        "ts-protoc-gen": "^0.15.0",
        "typescript": "^5.9.3"
    }
